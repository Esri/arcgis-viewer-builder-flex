<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009"
         xmlns:s="library://ns.adobe.com/flex/spark"
         xmlns:views="com.esri.builder.views.*"
         height="20">
    <s:Group left="0" right="0" top="0" bottom="0">
        <s:Line left="0" right="0" bottom="0">
            <s:stroke>
                <s:SolidColorStroke caps="none"
                                    color="0xB6BCCC"
                                    joints="miter"
                                    miterLimit="10"
                                    weight="2"/>
            </s:stroke>
        </s:Line>
        <s:Rect left="0" right="0" top="0" bottom="1">
            <s:fill>
                <s:LinearGradient x="492" y="0"
                                  rotation="90"
                                  scaleX="17.739">
                    <s:GradientEntry color="0xFEFEFF" ratio="0"/>
                    <s:GradientEntry color="0xE1E6F6" ratio="1"/>
                </s:LinearGradient>
            </s:fill>
        </s:Rect>
    </s:Group>
    <views:BreadcrumbTrail left="20" verticalCenter="0"/>
    <views:HeaderMenu right="20" verticalCenter="0"/>
</s:Group>

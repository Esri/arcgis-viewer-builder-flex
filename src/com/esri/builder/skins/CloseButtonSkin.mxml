<s:Skin xmlns:s="library://ns.adobe.com/flex/spark"
        xmlns:fx="http://ns.adobe.com/mxml/2009"
        width="16" height="16">
    <fx:Metadata>[HostComponent("spark.components.Button")]</fx:Metadata>
    <s:states>
        <s:State name="up"/>
        <s:State name="over"/>
        <s:State name="down"/>
        <s:State name="disabled"/>
    </s:states>
    <s:Rect x.disabled="0" x.down="0" x.up="0" y.disabled="0" y.down="0" y.up="0" width="16" height="16"
            left.over="0" top.over="0">
        <s:fill>
            <s:SolidColor color="#333333"/>
        </s:fill>
    </s:Rect>
    <s:Rect left="0" right="0" top="0" bottom="0"
            visible.over="false">
        <s:fill>
            <s:SolidColor color="#9B9B9B" color.disabled="#CCCCCC"/>
        </s:fill>
    </s:Rect>
    <s:Group left="2" top="2">
        <s:Line x="1.5" y="1.5"
                xTo="9"
                yTo="9">
            <s:stroke>
                <s:SolidColorStroke caps="none"
                                    color="#FFFFFF"
                                    joints="miter"
                                    miterLimit="10"
                                    weight="3"/>
            </s:stroke>
        </s:Line>
        <s:Line x="1.5" y="1.5"
                xTo="9"
                yFrom="9">
            <s:stroke>
                <s:SolidColorStroke caps="none"
                                    color="#FFFFFF"
                                    joints="miter"
                                    miterLimit="10"
                                    weight="3"/>
            </s:stroke>
        </s:Line>
    </s:Group>
</s:Skin>
